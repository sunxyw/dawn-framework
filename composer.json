    "require": {
        "php": "^8.1",
        "httpsoft/http-message": "^1.0",
        "httpsoft/http-response": "^1.0",
        "httpsoft/http-server-request": "^1.0",
        "slim/http": "^1.2",
        "slim/slim": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
